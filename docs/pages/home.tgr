					/block

					block : class[item] onclick[filterMedia(this, 'media_search_query_tv')]
						:: TV Shows
					/block

					block : class[item] onclick[filterMedia(this, 'media_search_query_movie')]
						:: Movies
					/block
	/item



			item : id[accountDetectedWrapper]

				block : class[section-divider]
				/block



				block : class[item]
