title : Baileo - Home

nav-block : class[left-navbar]

	block : class[logo-display]
		image : images/logo.png
		item : class[logo-text]
			:: baileo
		/item
	/block

	item : id[media_filter]
					block : class[item active] onclick[filterMedia(this, 'media_search_query')] id[allMediaFilter]
