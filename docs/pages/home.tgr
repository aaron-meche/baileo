					/item
				/block

				block : class[item] onclick[open_panel('continue watching')]
					:: Continue Watching
				/block

				block : class[item] id[logoutButton]
					:: Log Out
				/block

			/item



					item : id[noAccountDetectedWrapper] style[display: none;]
					
						block : class[section-divider]
						/block



						block : class[item] id[signInGoogleButton]
							:: Sign in to an account
						/block

					/item



	block : class[section-divider]
	/block



	block : class[item] onclick[open_panel('categories')]
		:: Categories
	/block

	block : class[item] onclick[open_panel('random_picker')]
		:: Pick Something for Me
	/block

/block




